    <style>
      /* Reset and Base Styles */
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: "Arial", sans-serif;
      }

      body {
        line-height: 1.6;
        color: #333;
        background: #f5f5f5;
        display: flex;
        flex-direction: column;
        min-height: 100vh;
      }

      /* Main Content Styles */
      .locations-container {
        flex: 1;
        max-width: 1200px;
        margin: 2rem auto;
        padding: 0 20px;
        width: 100%;
      }

      .page-title {
        text-align: center;
        color: #e74c3c;
        margin: 2rem 0;
        font-size: 2.5rem;
      }

      .locations-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 1.5rem;
        padding: 0 20px;
      }

      .location-card {
        background: #fff;
        border-radius: 10px;
        padding: 1.5rem;
        box-shadow: 0 3px 10px rgba(0, 0, 0, 0.1);
        transition: transform 0.3s ease;
      }

      .location-card:hover {
        transform: translateY(-5px);
      }

      .location-card h3 {
        color: #2c3e50;
        margin-bottom: 1rem;
        font-size: 1.3rem;
      }

      .location-card p {
        margin-bottom: 0.5rem;
        color: #555;
      }

      .contact-info {
        margin-top: 1rem;
        padding-top: 1rem;
        border-top: 1px solid #eee;
      }

      .contact-info a {
        color: #e74c3c;
        text-decoration: none;
        font-weight: bold;
      }

      .direction-link {
        margin-left: 15px;
        text-decoration: none;
        color: #0066cc;
        display: inline-flex;
        align-items: center;
        gap: 5px;
      }
      .direction-link:hover {
        text-decoration: underline;
      }
      .direction-link svg {
        transform: rotate(90deg);
      }
      /* Responsive Design */
      @media (max-width: 768px) {
        .page-title {
          font-size: 2rem;
        }

        .locations-grid {
          grid-template-columns: 1fr;
          padding: 0;
        }
      }

      @media (max-width: 480px) {
        .mobile-menu {
          display: block;
        }

        .location-card {
          padding: 1rem;
        }
      }
    </style>
  </head>
